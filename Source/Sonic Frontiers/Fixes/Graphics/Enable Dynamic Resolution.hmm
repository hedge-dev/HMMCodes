//
    #lib "Memory"
//
{
    Memory.WriteString(0x141741140, ">FrameStart");
    WriteProtected<byte>(0x14174114B, 0);
    Memory.WriteString(0x141741170, ">FrameEnd");
    WriteProtected<byte>(0x141741179, 0);
    WriteNop(0x1410F6F36, 6);
    WriteNop(0x1410F6F6C, 15);
}
